	$(CC) -o $(output2)_ptr-to-double ./ptr-to-double/$(output2)_ptr-to-double.c ./ptr-to-double/$(source21)_ptr-to-double.c ./ptr-to-double/$(source2)_ptr-to-double.c ./ptr-to-double/$(source3).c

clean:
	rm $(output1)_int $(output2)_int
	rm $(output1)_short $(output2)_short
	rm $(output1)_long $(output2)_long
	rm $(output1)_char $(output2)_char
	rm $(output1)_float $(output2)_float
	rm $(output1)_double $(output2)_double
	rm $(output1)_ptr-to-int $(output2)_ptr-to-int
	rm $(output1)_ptr-to-short $(output2)_ptr-to-short
	rm $(output1)_ptr-to-long $(output2)_ptr-to-long
	rm $(output1)_ptr-to-char $(output2)_ptr-to-char
	rm $(output1)_ptr-to-float $(output2)_ptr-to-float
	rm $(output1)_ptr-to-double $(output2)_ptr-to-double

